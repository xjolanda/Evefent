<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    package="kzooevefent.com.evefent" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".RegistrationScreen"
            android:label="RegistrationScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN">
                </action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>

        <activity android:name=".SampleDynamicInfoActivity"
            android:label="SampleDynamicInfoActivity"></activity>

        <service android:name=".DatabaseServices"
                 android:enabled="true"
                 android:exported="true">
        </service>

    </application>

</manifest>
